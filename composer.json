{
    "name": "liqwiz/laravel-sso-client",
    "description": "Laravel SSO client package - turn any Laravel app into an SSO client using a Hub (OAuth2 IdP)",
    "keywords": ["laravel", "sso", "oauth", "single sign-on", "passport"],
    "license": "MIT",
    "authors": [
        {
            "name": "Liqwiz",
            "email": "dev@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/config": "^10.0|^11.0|^12.0",
        "illuminate/auth": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0|^11.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Liqwiz\\LaravelSsoClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Liqwiz\\LaravelSsoClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Liqwiz\\LaravelSsoClient\\SsoClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
